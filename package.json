{
  "name": "fftf-starter-template",
  "version": "1.0.0",
  "description": "A Fight for the Future website",
  "author": "Fight for the Future <team@fightforthefuture.org>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "pregenerate": "npm run generate-leaderboard",
    "generate": "nuxt generate",
    "generate-leaderboard": "node scripts/generate-leaderboard.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "analyze": "nuxt build --analyze",
    "deploy": "rsync -avh dist/ fftf@static1.fightforthefuture.org:~/www/defendcrypto.org --delete"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.3.0",
    "clipboard": "^2.0.4",
    "cross-env": "^5.2.0",
    "nuxt": "^2.6.3",
    "nuxt-i18n": "^5.10.0",
    "nuxt-vue-multiselect": "^1.0.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/style-resources": "^0.1.2",
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "cryptocurrencies": "^7.0.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": ">=2.14.0",
    "eslint-plugin-jest": ">=21.24.1",
    "eslint-plugin-node": ">=7.0.1",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "exif-js": "^2.3.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0",
    "yaml-loader": "^0.5.0"
  }
}
